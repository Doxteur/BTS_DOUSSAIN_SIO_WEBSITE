: #ffb900;\r\n  background-color: #ffb900;\r\n}\r\n\r\n\r\n#footerdiv{\r\n  background-color: red;\r\n  height: 10vh;\r\n}\r\n\r\n\r\n\r\n/* Le Loader */\r\n.loader-wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  background-color: #1F1F1F;\r\n  display:flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.loader {\r\n  display: inline-block;\r\n  width: 30px;\r\n  height: 30px;\r\n  position: relative;\r\n  border: 4px solid #f5cf09;\r\n  animation: loader 2s infinite ease;\r\n}\r\n.loader-inner {\r\n  vertical-align: top;\r\n  display: inline-block;\r\n  width: 100%;\r\n  background-color: #fff;\r\n  animation: loader-inner 2s infinite ease-in;\r\n}\r\n@keyframes loader {\r\n  0% { transform: rotate(0deg);}\r\n  25% { transform: rotate(180deg);}\r\n  50% { transform: rotate(180deg);}\r\n  75% { transform: rotate(360deg);}\r\n  100% { transform: rotate(360deg);}\r\n}\r\n@keyframes loader-inner {\r\n  0% { height: 0%;}\r\n  25% { height: 0%;}\r\n  50% { height: 100%;}\r\n  75% { height: 100%;}\r\n  100% { height: 0%;}\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash":"2c86b369fbf6ff6da6d024896fdaccff","cacheData":{"env":{}}}